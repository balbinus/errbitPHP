{
    "name": "emgiezet/errbit-php",
    "type": "library",
    "description": "errbit/airbrake integration with php with phpcs-2",
    "keywords": ["errbit", "errbit php", "error tracking", "airbrake"],
    "homepage": "http://github.com/emgiezet/errbitPHP",
    "license": "MIT",
    "authors": [
        {
            "name": "flippa"
        },
        {
            "name": "emgiezet"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit/phpcov": "2.0.*@dev",
        "mockery/mockery": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "replace": {
        "nodrew/php-airbrake": "dev-master",
        "flippa-official/errbit-php": "dev-master"
    },
    "autoload": {
        "psr-0": { 
            "Errbit\\": "src/"
         }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
